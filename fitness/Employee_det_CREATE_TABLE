CREATE TABLE Employee_det(
   Emp_id int,
   emp_name varchar(50),
   Gender varchar(20),
   City varchar(20),
   Mpin int,
   Social_Mode Varchar(20),
   Mobile_num Varchar(20),
   Age int,
   Height int,
   Weight int,
   Medical_history varchar(20),
   Occupation varchar(20),
   Activity_level varchar(20),
   Rewards int,
   User_Name Varchar(20),
   Password Varchar(20),
   PRIMARY KEY( Emp_id )
);
